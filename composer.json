{
    "name": "aulasoftwarelibre/ddd-base",
    "description": "Base classes for DDD projects",
    "authors": [
        {
            "name": "Sergio Gómez",
            "email": "sergio@uco.es"
        },
        {
            "name": "Víctor Monserrat",
            "email": "victormonserratvillatoro@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "prooph/common": "^4.2",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store-symfony-bundle": "^0.5"
    },
    "require-dev": {
        "doctrine/collections": "^1.5",
