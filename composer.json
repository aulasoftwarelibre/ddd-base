{
    "name": "aulasoftwarelibre/ddd-base",
    "description": "Base classes for DDD projects",
    "authors": [
        {
            "name": "Sergio Gómez",
            "email": "sergio@uco.es"
        },
        {
            "name": "Víctor Monserrat",
            "email": "victormonserratvillatoro@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.8",
        "doctrine/orm": "^2.6",
        "prooph/common": "^4.2",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store": "^7.3",
        "prooph/event-store-symfony-bundle": "^0.5",
        "prooph/pdo-event-store": "^1.9",
        "prooph/pdo-snapshot-store": "^1.5",
        "prooph/snapshotter": "^2.1",
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/messenger": "^4.2",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AulaSoftwareLibre\\DDD\\BaseBundle\\": "src/"
        }
    }
}
